{
  "version": 2,
  "builds": [
    {
      "src": "api/check-digicode.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/appointments.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/send-email.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test-email.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/check-digicode",
      "dest": "/api/check-digicode.js"
    },
    {
      "src": "/api/appointments",
      "dest": "/api/appointments.js"
    },
    {
      "src": "/api/resend",
      "dest": "/api/send-email.js"
    },
    {
      "src": "/api/send-email",
      "dest": "/api/send-email.js"
    },
    {
      "src": "/api/test-email",
      "dest": "/api/test-email.js"
    },
    {
      "src": "/api/(.*)",
      "status": 404
    },
    {
      "src": "/site\\.webmanifest$",
      "dest": "/site.webmanifest"
    },
    {
      "src": "/robots\\.txt$",
      "dest": "/robots.txt"
    },
    {
      "src": "/sitemap\\.xml$",
      "dest": "/sitemap.xml"
    },
    {
      "src": "/favicon\\.png$",
      "dest": "/favicon.png"
    },
    {
      "src": "/apple-touch-icon\\.png$",
      "dest": "/apple-touch-icon.png"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/404",
      "dest": "/404.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}